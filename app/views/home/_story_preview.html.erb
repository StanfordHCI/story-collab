<% cache(story) do %>
  <%= link_to story, :class => "story-preview" do %>
    <h1><%= story.title %> 
      <small class="author">
        Started by <%= story.user.name %>
      </small>
    </h1>
    <div class="last-updated">
      Started <%= time_ago_in_words(story.created_at) %> ago
    </div>
  <% end %>
<% end %>
